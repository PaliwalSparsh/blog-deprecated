{
  "name": "next-blog",
  "version": "1.0.0",
  "description": "It's not a website, it's an experience.",
  "scripts": {
    "dev": "NODE_ENV=development next",
    "build": "NODE_ENV=production next build",
    "build-dev": "NODE_ENV=development next build",
    "start": "NODE_ENV=production next start",
    "clean": "rm -rf .next ; npm ci ; NODE_ENV=development npm run dev",
    "now-build": "next build && next export -o dist",
    "test": "jest"
  },
  "keywords": [
    "next.js",
    "react.js",
    "blog",
    "mdx"
  ],
  "author": "Sparsh Paliwal",
  "license": "ISC",
  "bin": {
    "blog": "cli.js"
  },
  "browserslist": [
    "last 2 version"
  ],
  "dependencies": {
    "@mdx-js/loader": "^0.15.6",
    "@zeit/next-mdx": "^1.2.0",
    "babel-plugin-preval": "^3.0.1",
    "chalk": "^2.4.1",
    "git-branch-is": "^3.0.0",
    "js-yaml": "^3.12.0",
    "js-yaml-loader": "^1.0.1",
    "next": "^9.3.2",
    "next-fonts": "^0.10.0",
    "next-seo": "^1.2.0",
    "react": "^16.8.2",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.8.2",
    "react-icons": "^3.2.2",
    "react-syntax-highlighter": "^9.0.1",
    "remark-emoji": "^2.0.2",
    "remark-images": "^0.15.6",
    "scrollprogress": "^3.0.2",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "husky": "^3.0.1",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^23.6.0",
    "next-optimized-images": "^2.0.1",
    "svg-sprite-loader": "^4.1.3",
    "webp-loader": "^0.4.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "if git-branch-is master; then now deploy; fi"
    }
  }
}
